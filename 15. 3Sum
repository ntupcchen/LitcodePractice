#Python3
#Medium difficulty

class Solution:
    def threeSum(self, nums: List[int]) -> List[List[int]]:
        sol=[]

        for i in range(len(nums)):
          for j in range(i+1,len(nums)):
            if (nums[i]+nums[j])*-1 in nums[j+1:]:
              add=[nums[i],nums[j],-nums[i]-nums[j]]
              add.sort()
              if add not in sol:
                sol.append(add)

        return sol
        
